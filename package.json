{
  "name": "vite-plugin-subresource-integrity",
  "private": false,
  "version": "0.0.5",
  "type": "module",
  "description": "Subresource integrity (SRI) plugin for Vite",
  "author": "jadsn <jadsn1894@protonmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JADSN1894/vite-plugin-sri.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup ./src/index.ts --watch --ignore-watch examples --format cjs,esm --dts --clean",
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean --minify",
    "test": "vitest"
  },
  "homepage": "https://github.com/JADSN1894/vite-plugin-sri#readme",
  "bugs": {
    "url": "https://github.com/JADSN1894/vite-plugin-sri/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "Vite",
    "Subresource",
    "Integrity",
    "SRI",
    "Plugin"
  ],
  "devDependencies": {
    "@types/cheerio": "0.22.31",
    "@types/node": "20.4.5",
    "cheerio": "1.0.0-rc.12",
    "tsup": "7.1.0",
    "typescript": "5.1.6",
    "vite": "4.4.8",
    "vitest": "0.34.1"
  }
}
